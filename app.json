{
  "expo": {
    "name": "cheskoslot",
    "slug": "cheskoslot",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/iconslot.png",
    "scheme": "cheskoslot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.chesko.slot777",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CHANGE_WIFI_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/futuristic-casino-architecture.jpg",
        "backgroundColor": "#0066"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/iconslot.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iconslot.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0066"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": ""
      }
    }
  }
}
